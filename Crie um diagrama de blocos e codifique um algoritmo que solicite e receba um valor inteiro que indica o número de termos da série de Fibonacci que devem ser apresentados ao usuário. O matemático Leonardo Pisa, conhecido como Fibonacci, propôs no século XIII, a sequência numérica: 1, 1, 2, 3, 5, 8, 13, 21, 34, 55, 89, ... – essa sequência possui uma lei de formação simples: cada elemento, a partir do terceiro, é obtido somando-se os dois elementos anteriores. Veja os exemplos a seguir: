programa
{
	
	funcao inicio()
	{
		inteiro numero,o,num1 = 1, num2 = 0
		escreva("Numero de termos:")
		leia(numero)
		numero = numero - 1
		escreva("\n-serie de Fibonacci[",numero + 1,"]: 1 ") 
		para(o = 0;o < numero;o++)
          {
          	num1 = num1 + num2
            	num2 = num1 - num2
            	escreva(num1," ")
          }
	}
}
